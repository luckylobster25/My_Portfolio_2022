<!-- We are not counting the number of times we visit this page -->
<!-- But we are showing the number of times we visited the homepage -->
<section class="container">
  <h3>Number of times homepage is visited: {{countVisit}}</h3>
  <section class="painting">
    <img src="/images/{{painting.filename}}" alt="{{painting.optional}}">
    <div class="painting-details">
      <h3>{{painting.title}}</h3>
      <ul>
        <li class="listlink"><span class="linkbox">LINK #1</span> <a href="{{painting.link}}">{{painting.link}}</a></li>
        <li class="listlink"><span class="linkbox">LINK #2</span> <a href="{{painting.optional}}">{{painting.optional}}</a></li>
      </ul>
      <div class="discription-box">
        <p class="project-discription"><span class="description-text">{{painting.textBox}}</span>
        </p>
  </section>
</section>